{
  "name": "yellow-three/visionary",
  "type": "yt-extension",
  "description": "YT Theme",
  "keywords": [
    "yt",
    "framework",
    "laravel",
    "visionary",
    "theme"
  ],
  "authors": [
    {
      "name": "Abdurrahman Harmankaya",
      "email": "abdurrahman.kim@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.9",
    "livewire/livewire": "^3.4"
  },
  "require-dev": {
    "laravel/pint": "^1.13"
  },
  "autoload": {
    "psr-4": {
      "YellowThree\\Visionary\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [],
      "providers": [
        "YellowThree\\Visionary\\Providers\\VisionServiceProvider"
      ],
      "aliases": {
        "Visionary": "YellowThree\\Visionary\\Facades\\Visionary"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT"
}
